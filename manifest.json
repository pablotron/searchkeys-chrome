{
  "name": "Chrome Search Keys",
  "version": "1.0",
  "description": "Select search results by number and navigate pages of search results using ',' and '.'.",

  "content_scripts": [{
    "matches": [
      "http://www.google.com/search*",
      "https://www.google.com/search*",

      "http://www.bing.com/search*",
      "https://www.bing.com/search*",

      "http://search.yahoo.com/search*"
    ],

    "run_at": "document_end",

    "css": ["style.css"],
    "js":  ["searchkeys.js"]
  }]
}
